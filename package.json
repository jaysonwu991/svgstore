  "license": "MIT",
  "main": "src/svgstore.js",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "lint": "eslint --fix \"{src,test}/**/*.ts\" && prettier --write \"{src,test}/**/*.ts\"",
